{
  "name": "workspace-service",
  "version": "0.0.1",
  "main": "index.js",
  "repository": "https://github.com/capsulajs/workspace-service.git",
  "author": "stephane Benayoun <stephane.benayoun@jivygroup.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start": "parcel src/index.html --target browser --no-cache",
    "build:web": "yarn run parcel build src/webComponents/EnvDropdown.tsx --target esm --no-cache",
    "lint": "tslint -p tsconfig.json --fix",
    "prettier": "prettier --write '{src,tests}/**/*.{ts,tsx}'",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "jest": {
    "preset": "ts-jest",
    "transform": {
      "\\.ts$": "ts-jest"
    },
    "testRegex": ".*\\.test\\.ts$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "./tests/setup.ts"
    ],
    "globals": {},
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/tests/__mocks__/fileMock.ts",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "dependencies": {
    "@babel/preset-react": "^7.0.0",
    "@capsulajs/capsulahub-ui": "^0.2.7",
    "@capsulajs/environment-registry": "^0.0.1",
    "@scalecube/scalecube-microservice": "next",
    "@types/react": "^16.8.12",
    "@types/react-dom": "^16.8.3",
    "react": "^16.8.5",
    "react-dom": "^16.8.5",
    "rxjs": "^6.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/polyfill": "^7.4.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@types/jest": "^24.0.6",
    "@types/node": "^11.13.2",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.5",
    "codacy-coverage": "^3.4.0",
    "document-register-element": "^1.13.1",
    "husky": "^1.3.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.1.0",
    "lodash": "^4.17.11",
    "parcel-bundler": "^1.9.7",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "rollup": "^1.8.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-babel-minify": "^8.0.0",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-node-resolve": "^4.1.0",
    "rollup-plugin-typescript3": "^1.1.2",
    "ts-jest": "^24.0.0",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.4.1"
  }
}
